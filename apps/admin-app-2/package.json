  "private": true,
  "description": "Admin Panel Frontend",
  "engines": {
    "npm": "^8.1.2",
    "node": "^16.13.2"
  },
  "author": "Getir Panel Development Team",
  "scripts": {
    "prestart": "node ./internals/scripts/checkNodeVersion.js",
    "start": "NODE_ENV=development REACT_APP_ENV=local webpack serve --mode development --config ./webpack.dev.config.js",
    "dev": "NODE_ENV=development REACT_APP_ENV=local webpack serve --mode development --config ./webpack.dev.config.js --port 9001",
    "start-4": "NODE_OPTIONS=--max_old_space_size=4096 npm run start",
    "build": "NODE_ENV=production NODE_OPTIONS=--max_old_space_size=5120 webpack --mode production --color --progress --config ./webpack.prod.config.js",
    "serve-build": "http-server ./build -p 9003 --gzip --proxy http://localhost:9003?",
    "build:analyze": "npm run build -- --analyze",
    "docker:prod:down": "docker-compose down",
    "docker:prod": "docker-compose up -d --build",
    "type-check": "NODE_OPTIONS=--max_old_space_size=4096 tsc",
    "create-diff-tsconfig": "node ./internals/scripts/createTsConfigForDiff.js",
    "lint-app": "eslint './app/**/*.{js,jsx,ts,tsx}' './internals/**/*.{js,jsx,ts,tsx}' './webpack*.{js,jsx,ts,tsx}' --quiet",
    "lint-app-txt": "mkdir -p npm_script_outputs; npm run lint-app > ./npm_script_outputs/eslint_output.txt",
    "lint-app-html": "mkdir -p npm_script_outputs; npm run lint-app -- -f html -o ./npm_script_outputs/eslint_output.html",
    "print-eslint-config": "mkdir -p npm_script_outputs; eslint --print-config ./app/index.js > ./npm_script_outputs/eslint_config.json",
    "lint-files": "eslint --quiet",
    "eslint-fix": "eslint './app/**/*.{js,jsx,ts,tsx}' './internals/**/*.{js,jsx,ts,tsx}' --fix",
    "lint-staged": "lint-staged",
    "test": "NODE_ENV=test jest -c ./jest.config.js --runInBand",
    "test-coverage": "NODE_ENV=test npm run test -- --coverage",
    "test-ci": "NODE_ENV=test npm run test-coverage -- --silent --ci --bail",
    "test-watch": "NODE_ENV=test npm run test -- --watch",
    "test-changed": "NODE_ENV=test COVERAGE_THRESHOLD_ENABLED=true npm run test-coverage -- --changedSince=origin/master --silent",
    "test-e2e": "npx playwright test",
    "test-e2e-smoke": "npx playwright test --grep @smoke",
    "test-e2e-ui": "npx playwright test --ui",
    "test-e2e-headed": "E2E_WORKERS=1 E2E_SLOW_MO=200 npx playwright test --headed",
    "test-e2e-record": "npx playwright codegen",
    "test-e2e-squad": "E2E_WORKERS=1 npx playwright test --grep $(node ./internals/scripts/outputSquadTestPattern.js)",
    "sort": "node ./internals/scripts/sortJsonFiles.js",
    "check-translations": "node ./internals/scripts/checkTranslationFiles.js",
    "generate": "plop --plopfile internals/generators/index.js"
  },
